	<queryString><![CDATA[select incident.createdate as thedate,incident.stationcreated_ID as stationcode,
count(incident.incident_ID) as reportstaken,
count(closeinc.incident_ID) as reportsclosed,
avg(incident.incident_ID) as recoveryratio from
incident incident left outer join incident closeinc 
on incident.incident_ID = closeinc.incident_ID 
and closeinc.status_ID = 4 
group by incident.stationcreated_ID
order by incident.stationcreated_ID]]></queryString>

