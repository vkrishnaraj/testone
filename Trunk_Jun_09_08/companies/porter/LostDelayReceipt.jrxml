				</textElement>
				<text><![CDATA[Damaged Baggage or Contents]]></text>
			</staticText>
			<staticText>
				<reportElement key="textField-48" positionType="Float" mode="Opaque" x="0" y="674" width="542" height="22" isPrintWhenDetailOverflows="true" forecolor="#000000" backcolor="#FFFFFF"/>
