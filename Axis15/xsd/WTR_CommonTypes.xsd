				</xs:complexType>
			</xs:element>
			<xs:element name="FaultTerminal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>FT - Terminal  responsible for the mishandling of the bag</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="AlphaNumericStringLength0or2">
							<xs:attribute name="Delete" type="xs:boolean" use="optional" default="false"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Insurance" minOccurs="0">
				<xs:annotation>
					<xs:documentation>IN - Indicates whether the passenger has baggage insurance</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="EmptyType">
							<xs:attribute name="Delete" type="xs:boolean" use="optional" default="false"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="LiabilityTag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>LT - Liability tag</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="EmptyType">
							<xs:attribute name="Delete" type="xs:boolean" use="optional" default="false"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="MissingBags" minOccurs="0">
				<xs:annotation>
					<xs:documentation>MB - Contains number of bags the claimant reports missing</xs:documentation>
