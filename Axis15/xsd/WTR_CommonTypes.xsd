					<xs:choice>
						<xs:element name="CarrierCode">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:pattern value="[a-zA-Z0-9]{2,3}"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
