	<xs:complexType name="RecordIdentifierType">
		<xs:annotation>
			<xs:documentation>This identifies a unique bag record in the WorldTracer System</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="RecordType" type="RecordType"/>
			<xs:element name="RecordReference" type="RecordReferenceType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RecordIdentifierAmendType">
		<xs:annotation>
			<xs:documentation>This identifies a unique bag record in the WorldTracer System</xs:documentation>
		</xs:annotation>
		<xs:sequence minOccurs="0">
			<xs:element name="RecordType" type="RecordType"/>
			<xs:element name="RecordReference" type="RecordReferenceType"/>
		</xs:sequence>
		<xs:attribute name="Delete" type="xs:boolean" use="optional" default="false"/>
	</xs:complexType>
	<xs:complexType name="RecordReferenceType">
		<xs:annotation>
			<xs:documentation>This contains unique referece string to identify a particular bag type</xs:documentation>
		</xs:annotation>
		<xs:attributeGroup ref="StationAirlineGroup"/>
		<xs:attribute name="ReferenceNumber">
			<xs:simpleType>
				<xs:restriction base="xs:int">
					<xs:maxInclusive value="999999"/>
					<xs:minInclusive value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="StationAirlineType">
		<xs:annotation>
			<xs:documentation>Contains station airline pair</xs:documentation>
		</xs:annotation>
		<xs:attribute name="StationCode" type="iata:StationType"/>
		<xs:attribute name="AirlineCode" type="iata:AirlineType"/>
	</xs:complexType>
	<xs:complexType name="ColorTypeDescType">
		<xs:annotation>
			<xs:documentation>Contains color type of the bag</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ColorCode" type="ColorCodeType"/>
			<xs:element name="TypeCode" type="BagTypeCodeType"/>
			<xs:element name="Descriptor" type="BagDescType"/>
		</xs:sequence>
		<xs:attribute name="Suspended" type="xs:boolean"/>
	</xs:complexType>
	<xs:complexType name="BagTagType">
		<xs:annotation>
			<xs:documentation>Contains Bag Tag type details</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="AirlineCode" type="iata:AirlineType"/>
				<xs:element name="LicenseNumber" type="LicenseNumberType"/>
			</xs:choice>
			<xs:element name="TagSequence" type="TagSequenceType"/>
		</xs:sequence>
		<xs:attribute name="Suspended" type="xs:boolean"/>
	</xs:complexType>
	<xs:complexType name="FlightType">
		<xs:annotation>
			<xs:documentation>Contains Flight and Date</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AirlineCode" type="iata:AirlineType"/>
			<xs:element name="FlightNumber" type="FlightNumberType"/>
			<xs:element name="Date" type="xs:date"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LicenseNumberType">
		<xs:attribute name="TagType" type="NumericLength1" use="optional"/>
		<xs:attribute name="IssuerCode" type="IssuerCodeType" use="required"/>
	</xs:complexType>
	<xs:complexType name="BagDescType">
		<xs:annotation>
			<xs:documentation>Maximum of the total elements should be 3. 1 MtrlElement + 2 OtherElement or 3 OtherElement</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="MtrlElement" type="BagMatrlType" minOccurs="0"/>
			<xs:element name="OtherElement" type="BagElmsType" minOccurs="2" maxOccurs="3"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DiaryInfoType">
