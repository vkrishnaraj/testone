<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:iata="http://www.iata.org/IATA/2007/00" elementFormDefault="qualified" version="0.1" id="WTR2009A">
	<xs:include schemaLocation="WTR_SimpleTypes.xsd"/>
	<xs:import namespace="http://www.iata.org/IATA/2007/00" schemaLocation="IATA_ET_CommonTypes.xsd"/>
	<xs:complexType name="PassengerItineraryType">
		<xs:annotation>
			<xs:documentation>Contains details of a passenger like name, address etc</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PassengerType">
				<xs:sequence>
					<xs:element name="Itinerary">
						<xs:annotation>
							<xs:documentation>FD/RT - Flights, dates and routes the passenger travelled</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="FlightSegments">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="FlightSegmentOrARNK" type="FlightSegmentOrARNKType" maxOccurs="5">
												<xs:annotation>
													<xs:documentation>FD/BR/FO/NF - Flight Segment type or ARNK</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="AdditionalRoutes" minOccurs="0">
									<xs:annotation>
										<xs:documentation>RT - Additional route stations</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="Route" type="iata:StationType" maxOccurs="13"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="Status" type="StringLength1to25" minOccurs="0">
						<xs:annotation>
							<xs:documentation>PS - Class of service flown by the passenger</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PooledTktNumber" type="AlphaNumericStringLength1to15" minOccurs="0">
						<xs:annotation>
							<xs:documentation>PB - Pooled ticket number</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FareBasis" type="AlphaNumericStringLength1to7" minOccurs="0">
						<xs:annotation>
							<xs:documentation>PF - The fare basis as shown on the passengers flight ticket</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PassengerItineraryAmendType">
		<xs:annotation>
			<xs:documentation>Contains details of a passenger like name, address etc</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PassengerAmendType">
				<xs:sequence>
					<xs:element name="Itinerary" minOccurs="0">
						<xs:annotation>
							<xs:documentation>FD/RT - Flights, dates and routes the passenger travelled</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="FlightSegments" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="Flight" type="FlightSegmentOrARNK_AmendType" maxOccurs="5">
												<xs:annotation>
													<xs:documentation>FD/BR/FO/NF - Flight Segment type or ARNK</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="AdditionalRoutes" minOccurs="0">
									<xs:annotation>
										<xs:documentation>RT - Additional route stations</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="Route" type="iata:StationType" maxOccurs="13"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="Status" minOccurs="0">
						<xs:annotation>
							<xs:documentation>PS - Class of service flown by the passenger</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:extension base="StringLength0to58">
									<xs:attribute name="Delete" type="xs:boolean" default="false"/>
								</xs:extension>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="PooledTktNumber" minOccurs="0">
						<xs:annotation>
							<xs:documentation>PB - Pooled ticket number</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:extension base="AlphaNumericStringLength0to15">
									<xs:attribute name="Delete" type="xs:boolean" default="false"/>
								</xs:extension>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="FareBasis" minOccurs="0">
						<xs:annotation>
							<xs:documentation>PF - The fare basis as shown on the passengers flight ticket</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:extension base="AlphaNumericStringLength0to7">
									<xs:attribute name="Delete" type="xs:boolean" default="false"/>
								</xs:extension>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DeliveryType">
		<xs:annotation>
			<xs:documentation>Contians the deatials required for the delivery of the bag</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DeliveryAddress" minOccurs="0">
				<xs:annotation>
					<xs:documentation>DA - Delivery address required for baggage delivery</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AddressLine" type="StringLength0to58" maxOccurs="2"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="DeliveryWeight" type="AlphaNumericStringLength1to9" minOccurs="0">
				<xs:annotation>
					<xs:documentation>DW - Number and weight of the bags being delivered to the passenger</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LocalDlvInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>LD - Contains local delivery information and special instructions</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="DlvInfo" type="StringLength0to58" maxOccurs="2"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="DeliveryService" type="StringLength0to58" minOccurs="0" maxOccurs="10">
				<xs:annotation>
					<xs:documentation>DS - Delivery Service details. Display purpose only</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DeliveryInfo" type="StringLength0to2000" minOccurs="0" maxOccurs="10">
				<xs:annotation>
					<xs:documentation>DY - Delivery Information</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DeliveryAmendType">
		<xs:annotation>
			<xs:documentation>Contians the deatials required for the delivery of the bag</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DeliveryAddress" minOccurs="0">
				<xs:annotation>
					<xs:documentation>DA - Delivery address required for baggage delivery</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AddressLine" type="StringLength0to58AmendType" maxOccurs="2"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="DeliveryWeight" minOccurs="0">
				<xs:annotation>
					<xs:documentation>DW - Number and weight of the bags being delivered to the passenger</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="AlphaNumericStringLength0to9">
							<xs:attribute name="Delete" type="xs:boolean" use="optional" default="false"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="LocalDlvInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>LD - Contains local delivery information and special instructions</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="DlvInfo" type="StringLength0to58AmendType" maxOccurs="2"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="DeliveryService" type="StringLength0to58AmendType" minOccurs="0" maxOccurs="10">
				<xs:annotation>
					<xs:documentation>DS - Delivery Service details. Display purpose only</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ClaimType">
		<xs:annotation>
			<xs:documentation>Contains the claim data</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ClaimAmout" minOccurs="0">
				<xs:annotation>
					<xs:documentation>CA - Contains the passeneger estimated value of the lost or damaged items. Can hava value or unknown</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:element name="Unknown" type="EmptyType"/>
						<xs:element name="Amount">
							<xs:complexType>
								<xs:attributeGroup ref="iata:CurrencyAmountGroup"/>
							</xs:complexType>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element name="CostRemarks" minOccurs="0">
				<xs:annotation>
					<xs:documentation>CR - Contians an explanation of the cost payed to passenger (CS element)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="TextLine" type="StringLength1to58" maxOccurs="5"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="PassengerPayments" minOccurs="0">
				<xs:annotation>
					<xs:documentation>CS - All the costs paid to the passenger</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="PassengerPayment" type="PassengerPaymentType" maxOccurs="5"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ClaimDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>DC - Contains the date that the completed claim form is received</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DateNotified" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>DN - Date the claims deparment was notified</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="QuestionnaireDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>DQ - Quesstionnaire date</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FaultStationCode" type="iata:StationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>FS - Station responsible for the mishandling of the bag</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FaultTerminal" type="AlphaNumericStringLength2" minOccurs="0">
				<xs:annotation>
					<xs:documentation>FT - Terminal  responsible for the mishandling of the bag</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Insurance" type="EmptyType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>IN - Indicates whether the passenger has baggage insurance</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LiabilityTag" type="EmptyType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>LT - Liability tag</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MissingBags" type="NumericLength2" minOccurs="0">
				<xs:annotation>
					<xs:documentation>MB - Contains number of bags the claimant reports missing</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PartnerCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>PC - Contins the carrier code of the code share partner</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:element name="CarrierCode">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:pattern value="[a-zA-Z0-9]{2,3}"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="None"/>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element name="LossComments" type="StringLength1to58" minOccurs="0">
				<xs:annotation>
					<xs:documentation>RC - Contains comments regarding the reason for the loss of baggage</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ClaimAmendType">
