	<xs:complexType name="DelayedBagGroupType">
		<xs:complexContent>
			<xs:extension base="BagGroupType">
				<xs:sequence>
					<xs:element name="DelayedBags">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="DelayedBag" type="DelayedBagType" maxOccurs="10"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="BagLastSeen" type="iata:StationType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>BL - Station Bag last seen at</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BaggageItinerary" minOccurs="0">
						<xs:annotation>
							<xs:documentation>BR - Itenerary on the bags</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="FlightDateOrARNK" type="FlightDateOrARNKType" maxOccurs="5">
									<xs:annotation>
										<xs:documentation>FD/BR/FO/NF - Flight Segment type or ARNK</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="ExcessBaggage" type="AlphaNumericStringLength1to15" minOccurs="0">
						<xs:annotation>
							<xs:documentation>BX - Excess bagage Ticket Number</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BagTagDestinations" minOccurs="0">
						<xs:annotation>
							<xs:documentation>DB - Destination on BaggateTag</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Station" type="iata:StationType" maxOccurs="2"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="KeysCollected" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>KK - Indicates that the passengers baggage keys were collected or not</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MissingWeight" type="AlphaNumericStringLength1to9" minOccurs="0">
						<xs:annotation>
							<xs:documentation>NW - Total weight of the missing baggage</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MatchWindow" minOccurs="0">
						<xs:annotation>
							<xs:documentation>MW - Used as a qualifier to limit matches to only those On-Hand bags where the lowest date in filght falls with in the date range specified in the match window</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:int">
								<xs:minInclusive value="1"/>
								<xs:maxInclusive value="99"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
