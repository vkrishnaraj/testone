	<xs:complexType name="OnHandBagType">
		<xs:complexContent>
			<xs:extension base="BagType">
				<xs:sequence>
					<xs:element name="Itinerary">
						<xs:annotation>
							<xs:documentation>FD/RT - Flights, dates and routes the passenger travelled</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="FlightSegments">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="FlightSegment" type="FlightOptionalDateOrARNKType" maxOccurs="5"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="Routes">
									<xs:annotation>
										<xs:documentation>RT - Additional route stations</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="Route" type="iata:StationType" maxOccurs="5"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="BagAddress" minOccurs="0">
						<xs:annotation>
							<xs:documentation>AB - Addresses on bags</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="AddressLine" type="StringLength0to58" maxOccurs="2"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="BagContents" type="ContentsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>CC - Contians the description of the contents in a bag</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ContentsDesc" type="StringLength1to58" minOccurs="0">
						<xs:annotation>
							<xs:documentation>CN - Contains a description of dealyed or on-hand bags</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ContentsGender" type="GenderType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>CG - Contents Gender</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Remarks" minOccurs="0">
						<xs:annotation>
							<xs:documentation>RK - General remarks about the bag</xs:documentation>
						</xs:annotation>
