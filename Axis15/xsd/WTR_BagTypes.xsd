				<xs:annotation>
					<xs:documentation>CL - Cobination lock code of a bag</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BagAmendType">
		<xs:sequence>
			<xs:element name="ColorTypeDesc" type="ColorTypeDescAmendType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>CT - Contains color, type and description of a bag</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BagTag" type="BagTagAmendType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>TN - Contains baggage tag</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BrandInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>BI - Contains bag manufacturers name</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="StringLength0to58">
							<xs:attribute name="Delete" type="xs:boolean" use="optional" default="false"/>
