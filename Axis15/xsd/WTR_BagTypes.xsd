			</xs:element>
			<xs:element name="BagDelivery" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="BagReceivedDate" minOccurs="0">
							<xs:annotation>
								<xs:documentation>DR - Date and time the bag was received at the tracing station for forwarding to the passenger</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:date">
										<xs:attribute name="Delete" type="xs:boolean" use="optional" default="false"/>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="Status" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="OutForDelivery" minOccurs="0">
										<xs:annotation>
											<xs:documentation>OF - Out for delivery bag status</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="StringLength0to58">
													<xs:attribute name="Delete" type="xs:boolean" use="optional" default="false"/>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="Delivered" minOccurs="0">
										<xs:annotation>
											<xs:documentation>OD - Delivered bag status</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="StringLength0to58">
													<xs:attribute name="Delete" type="xs:boolean" use="optional" default="false"/>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="UnableToDeliver" minOccurs="0">
										<xs:annotation>
											<xs:documentation>UN - Unable to deliver bag status</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="StringLength0to58">
													<xs:attribute name="Delete" type="xs:boolean" use="optional" default="false"/>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="TrackingUpdate" minOccurs="0">
										<xs:annotation>
											<xs:documentation>UP - Status indicatign a tracking update</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="StringLength0to58">
													<xs:attribute name="Delete" type="xs:boolean" use="optional" default="false"/>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="DeliveredTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>DD - Date and time a bag is delivered</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:date">
										<xs:attribute name="Delete" type="xs:boolean" use="optional" default="false"/>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="LockCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>CL - Cobination lock code of a bag</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="AlphaNumericStringLength0to6">
							<xs:attribute name="Delete" type="xs:boolean" use="optional" default="false"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
