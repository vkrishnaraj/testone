							<xs:documentation>BL - Station Bag last seen at</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:extension base="AlphaLength0to4">
									<xs:attribute name="Delete" type="xs:boolean" use="optional" default="false"/>
								</xs:extension>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="BaggageItinerary" minOccurs="0">
						<xs:annotation>
							<xs:documentation>BR - Itenerary on the bags</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence minOccurs="0">
								<xs:element name="FlightDateOrARNK" type="FlightDateOrARNKType" maxOccurs="5">
									<xs:annotation>
										<xs:documentation>FD/BR/FO/NF - Flight Segment type or ARNK</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="Delete" type="xs:boolean" use="optional" default="false"/>
						</xs:complexType>
					</xs:element>
					<xs:element name="ExcessBaggage" minOccurs="0">
