<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>
 <converter>
  <converter-id>convertToUpper</converter-id>
  <converter-class>com.nettracer.claims.faces.util.StringAsUpper</converter-class>
 </converter>
  <converter>
  <converter-id>DateConverter</converter-id>
  <converter-class>com.nettracer.claims.faces.util.DateConverter</converter-class>
 </converter>
 <managed-bean>
  <managed-bean-name>sessionBean</managed-bean-name>
  <managed-bean-class>com.nettracer.claims.admin.SessionScopeBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>sessionPassengerBean</managed-bean-name>
  <managed-bean-class>com.nettracer.claims.passenger.SessionPassengerBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>requiredFieldsBean</managed-bean-name>
  <managed-bean-class>com.nettracer.claims.admin.RequiredFieldsBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>content</managed-bean-name>
  <managed-bean-class>com.nettracer.claims.core.model.Content</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>bag</managed-bean-name>
  <managed-bean-class>com.nettracer.claims.core.model.Bag</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <navigation-case>
   <from-outcome>gotoLandingPage</from-outcome>
   <to-view-id>/admin/landing.faces</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoContentsAndLanguage</from-outcome>
   <to-view-id>/admin/contentandlanguage.faces</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoRequiredFields</from-outcome>
   <to-view-id>/admin/requiredfields.faces</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoMaintainApplication</from-outcome>
   <to-view-id>/admin/maintainapplication.faces</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>logout</from-outcome>
   <to-view-id>/admin/login.faces</to-view-id>
  </navigation-case>
  <!-- Passenger side Navigation -->
  <navigation-case>
   <from-outcome>passengerLogout</from-outcome>
   <to-view-id>/claim/login.do</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoDirectionPage</from-outcome>
   <to-view-id>/claim/direction.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoPPFInstruction</from-outcome>
   <to-view-id>/claim/instruction.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoLiability</from-outcome>
   <to-view-id>/claim/liability.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoPassengerInfo</from-outcome>
   <to-view-id>/claim/passengerinfo.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoFlightDetails</from-outcome>
   <to-view-id>/claim/flightinfo.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoBagDetails</from-outcome>
   <to-view-id>/claim/baginfo.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoSubmitClaim</from-outcome>
   <to-view-id>/claim/submit-claim.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoSavedScreen</from-outcome>
   <to-view-id>/claim/saved-screen.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoFileUpload</from-outcome>
   <to-view-id>/claim/file-upload.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoFraudQuestion</from-outcome>
   <to-view-id>/claim/additional-info.do</to-view-id>
   <redirect/>
  </navigation-case>
  <!-- Mobile Navigation -->
  
  <navigation-case>
   <from-outcome>gotoMobileDirectionPage</from-outcome>
   <to-view-id>/claim/mobile/direction.do</to-view-id>
   <redirect/>
  </navigation-case>
  
  <!-- Self Serve Incident Navigation -->
  <navigation-case>
   <from-outcome>gotoIncidentLogin</from-outcome>
   <to-view-id>/incident/login.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoStepOnePage</from-outcome>
   <to-view-id>/incident/stepone.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoStepTwoPage</from-outcome>
   <to-view-id>/incident/steptwo.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoStepThreePage</from-outcome>
   <to-view-id>/incident/stepthree.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoStepFourPage</from-outcome>
   <to-view-id>/incident/stepfour.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoCompletePage</from-outcome>
   <to-view-id>/incident/complete.do</to-view-id>
   <redirect/>
  </navigation-case>
  
  <!--  Mobile Version -->
  <navigation-case>
   <from-outcome>gotoIncidentLoginMobile</from-outcome>
   <to-view-id>/incident/mobile/login.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoStepOnePageMobile</from-outcome>
   <to-view-id>/incident/mobile/stepone.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoStepTwoPageMobile</from-outcome>
   <to-view-id>/incident/mobile/steptwo.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoStepThreePageMobile</from-outcome>
   <to-view-id>/incident/mobile/stepthree.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoStepFourPageMobile</from-outcome>
   <to-view-id>/incident/mobile/stepfour.do</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoCompletePageMobile</from-outcome>
   <to-view-id>/incident/mobile/complete.do</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <factory>
	<exception-handler-factory>aero.nettracer.lfc.exception.ViewExpiredExceptionExceptionHandlerFactory</exception-handler-factory>
 </factory>
 <application>
  <variable-resolver>org.springframework.web.jsf.DelegatingVariableResolver</variable-resolver>
 </application>
</faces-config>
